{
  "name": "webpack-4-setup",
  "version": "1.0.0",
  "description": "setting webpack with modern tools",
  "private": true,
  "keywords": [
    "webpack",
    "babel",
    "react",
    "postcss",
    "autoprefixer",
    "hot-reloader"
  ],
  "author": "mario uhrin",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --env.development --open --config ./config/webpack.dev.js",
    "build": "webpack --env.production --config ./config/webpack.prod.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "**/*.{js,json,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.1",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "7.11.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.11.0",
    "node-sass-glob-importer": "^5.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "prettier-stylelint": "^0.4.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.7.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.2",
    "stylelint-selector-bem-pattern": "^2.0.0",
    "terser-webpack-plugin": "^1.1.0",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-hot-loader": "^4.3.12"
  },
  "browserslist": [
    "> 1%",
    "last 2 version"
  ]
